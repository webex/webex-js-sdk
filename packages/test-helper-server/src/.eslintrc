{
  "env": {
    "es6": false
  },
  "ecmaFeatures": {
    "blockBindings": false,
    "modules": false
  },
  "rules": {
    "arrow-parens": [0, "always"],
    "arrow-spacing": [0, {
      "after": true,
      "before": true
    }],
    "constructor-super": [0],
    "generator-star-spacing": [0],
    "no-class-assign": [0],
    "no-const-assign": [0],
    "no-dupe-class-members": [0],
    "no-this-before-super": [0],
    "no-var": [0],
    "object-shorthand": [0, "always"],
    "prefer-arrow-callback": [0],
    "prefer-const": [0],
    "prefer-reflect": [0],
    "prefer-spread": [0],
    "prefer-template": [0],
    "quotes": [0, "backtick"],
    "require-yield": [0]
  }
}
