{
  "LABEL": {
    "name": "title needs formatting",
    "color": "EEEEEE"
  },
  "CHECKS": {
    "prefixes": [
      "build: ",
      "chore: ",
      "ci: ",
      "docs: ",
      "feat: ",
      "fix: ",
      "perf: ",
      "refactor: ",
      "revert: ",
      "test: "
    ],
    "regexp": "(?<type>build|chore|ci|docs|feat|fix|perf|refactor|revert|test)(?<scope>(?:\\([^()\r\n]*\\)|\\()?(?<breaking>!)?:)(?<subject>:.*)?",
    "regexpFlags": "i",
    "ignoreLabels": ["dont-check-PRs-with-this-label", "meta"]
  },
  "MESSAGES": {
    "success": "All OK",
    "failure": "Failing CI test",
    "notice": ""
  }
}
