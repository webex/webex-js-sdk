  "scripts": {
    "precommit": "npm run --silent lint:staged",
    "commitmsg": "npm run --silent lint:commitmsg",
    "clean": "./tooling/index.js exec -- rimraf dist .coverage",
    "bootstrap": "echo 'npm run bootstrap is no longer required. Hooray!'",
    "prebuild": "./tooling/index.js exec -- rimraf dist",
    "build": "./tooling/index.js build",
    "prebuild:docs": "rimraf ./docs/api",
    "build:docs": "documentation build --config documentation/config.yml --np --project-name ciscospark --project-version 1.x --format html --output ./docs/api --github ./packages/node_modules/ciscospark/src/index.js ./packages/node_modules/@ciscospark/plugin-phone/src/index.js ./packages/node_modules/@ciscospark/plugin-people/src/index.js",
    "check-karma-output": "./scripts/analyze-output.sh",
    "build:package": "echo 'deprecated; just use \"npm run --silent build\"'./tooling/build.js",
    "lint": "npm run --silent lint:js && npm run --silent lint:jenkinsfile && npm run --silent lint:package-names",
    "lint:commitmsg": "conventional-changelog-lint -e",
    "lint:eslint": "eslint --ignore-pattern 'packages/node_modules/generator-ciscospark/generators/*/templates/**' --ignore-path .gitignore $(npm run --silent lint:eslint:ci-reporter)",
    "lint:eslint:ci-reporter": "bash -c '[[ -n \"${JENKINS}\" ]]' && echo \"-f checkstyle -o reports/style/eslint.xml\" || echo ''",
    "lint:jenkinsfile": "grep -qc '.status' Jenkinsfile && echo 'Use result, not status, in Jenkinsfile' || true",
    "lint:js": "npm run --silent lint:eslint -- .",
    "lint:package-names": "./tooling/lint-package-names.sh",
    "lint:staged": "lint-staged",
    "sauce:start": "./scripts/run-with-sauce.sh ./packages/node_modules/@ciscospark/bin-sauce-connect/bin/sauce-connect start",
    "sauce:stop": "./scripts/run-with-sauce.sh ./packages/node_modules/@ciscospark/bin-sauce-connect/bin/sauce-connect stop && rimraf .sauce/${PACKAGE}/sc.tid",
    "sauce:run": "./scripts/run-with-sauce.sh",
    "test": "./tooling/index.js test",
    "pretest:samples": "rimraf bundle.js bundle.js.map",
    "test:samples": "wdio",
    "distsrc": "sed -ibak 's#\"main\": \"dist/index.js#\"main\": \"src/index.js#' packages/node_modules/{*,@ciscospark/*}/package.json && rimraf packages/node_modules/{*,@ciscospark/*}/*bak",
    "srcdist": "sed -ibak 's#\"main\": \"src/index.js#\"main\": \"dist/index.js#' packages/node_modules/{*,@ciscospark/*}/package.json && rimraf packages/node_modules/{*,@ciscospark/*}/*bak",
    "prepublish": "in-install && npm run --silent link-lint-rules || not-in-install",
    "publish:docs": "grunt --gruntfile Gruntfile.concurrent.js publish:docs",
    "link-lint-rules": "rimraf ./node_modules/@ciscospark/eslint-config && mkdirp node_modules/@ciscospark && ln -s $(pwd)/packages/node_modules/@ciscospark/eslint-config $(pwd)/node_modules/@ciscospark/eslint-config",
    "get-next-version": "./tooling/index.js version next --always",
    "deps:check": "./tooling/index.js exec -- dependency-check ./package.json",
    "deps:generate": "./deps.js",
    "serve:samples": "webpack-dev-server",
    "tooling": "./tooling/index.js"
  },
