  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "build": "npm run --silent grunt:concurrent -- build",
    "check-karma-output": "./scripts/analyze-output.sh",
    "check-package": "if [ -z ${PACKAGE+x} ]; then echo 'environment variable PACKAGE must be defined' && exit 1; fi",
    "prebuild:package": "npm run --silent check-package",
    "build:package": "npm run --silent grunt:package -- build",
    "grunt": "grunt",
    "grunt:circle": "grunt --gruntfile Gruntfile.circle.js",
    "grunt:concurrent": "grunt --gruntfile Gruntfile.concurrent.js",
    "pregrunt:package": "npm run --silent check-package",
    "grunt:package": "grunt --gruntfile Gruntfile.package.js",
    "lint:jenkinsfile": "grep -qc '.status' Jenkinsfile && echo 'Use result, not status, in Jenkinsfile' || true",
    "lint:staged": "lint-staged",
    "release": "npm run grunt --silent -- release",
    "sauce:start": "./scripts/run-with-sauce.sh ./packages/bin-sauce-connect/bin/sauce-connect start",
    "sauce:stop": "./scripts/run-with-sauce.sh ./packages/bin-sauce-connect/bin/sauce-connect stop && rm -f .sauce/${PACKAGE}/sc.tid",
    "sauce:run": "./scripts/run-with-sauce.sh",
    "script": "bash -c",
    "test": "npm run --silent test:packages && npm run --silent test:legacy",
    "test:legacy-node": "UPLOAD_DIR=node npm run --silent grunt -- test:integration",
    "test:legacy-browser": "PACKAGE=legacy UPLOAD_DIR=browser npm run --silent grunt -- test:karma",
    "pretest:package": "npm run --silent check-package",
    "test:package": "npm run --silent grunt:package -- test --stack",
    "lerna": "lerna",
    "distsrc": "sed -ibak 's#\"main\": \"dist/index.js#\"main\": \"src/index.js#' packages/*/package.json && rm packages/*/*bak",
    "srcdist": "sed -ibak 's#\"main\": \"src/index.js#\"main\": \"dist/index.js#' packages/*/package.json && rm packages/*/*bak",
    "clean-empty-packages": "node ./scripts/clean-empty-packages.js --clean",
    "prepublish": "in-install && npm run --silent link-lint-rules || not-in-install",
    "link-lint-rules": "rm -rf ./node_modules/@ciscospark/eslint-config && mkdir -p node_modules/@ciscospark && ln -s $(pwd)/packages/eslint-config $(pwd)/node_modules/@ciscospark/eslint-config",
    "get-next-version": "node ./tooling/get-version.js",
    "depcheck": "npm run lerna --silent -- exec -- bash -c 'dependency-check ./package.json $(find ./test -name \"*.js\")'",
    "depcheck:outdated": "npm run lerna --silent -- exec -- npm outdated --depth 0"
  },
