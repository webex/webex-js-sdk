    "dependency-check": "^3.4.1",
    "detective": "^5.2.0",
    "detectrtc": "^1.3.4",
    "doctrine": "^2.1.0",
    "documentation": "^8.1.2",
    "documentation-theme-utils": "^3.0.0",
